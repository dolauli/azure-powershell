                    {
                        "name": "[-Location] <String>"
                    },
                    {
                        "name": "[-InRemovedState]"
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a deleted key vault in a resource group",
                    "arguments": [
                        {
                            "name": "-SubscriptionId",
                            "value": "[resources().subscriptionId]"
                        },
                        {
                            "name": "-InRemovedState",
                            "value": null
