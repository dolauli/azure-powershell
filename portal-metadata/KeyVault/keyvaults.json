    "commands": [
        {
            "name": "Get-AzKeyVault",
            "description": "Get a key vault",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/en-us/powershell/module/az.keyvault/get-azkeyvault"
                },
                "arguments": [
                    {
                        "name": "[-ResourceGroupName] <String>"
                    },
                    {
                        "name": "[[-VaultName] <String>]"
                    },
                    {
                        "name": "[-Tag <Hashtable>]"
                    },
                    {
                        "name": "[-SubscriptionId <String>]"
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a key vault in a resource group",
                    "arguments": [
                        {
                            "name": "-SubscriptionId",
                            "value": "[resources().subscriptionId]"
                        },
                        {
                            "name": "-ResourceGroupName",
                            "value": "[resources().resourceGroupName]"
                        },
                        {
                            "name": "-VaultName",
                            "value": "[resources().name]"
                        }
                    ]
                }
            ]
        },
